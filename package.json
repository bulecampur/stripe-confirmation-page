{
	"name": "my-qwik-basic-starter",
	"description": "Demo App with Routing built-in (recommended)",
	"engines": {
		"node": ">=15.0.0"
	},
	"private": true,
	"trustedDependencies": [
		"sharp"
	],
	"type": "module",
	"scripts": {
		"build": "qwik build",
		"build.client": "vite build",
		"build.preview": "vite build --ssr src/entry.preview.tsx",
		"build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
		"build.types": "tsc --incremental --noEmit",
		"deploy": "wrangler pages publish ./dist",
		"dev": "vite --mode ssr",
		"dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
		"fmt": "prettier --write .",
		"fmt.check": "prettier --check .",
		"lint": "eslint \"src/**/*.ts*\"",
		"preview": "qwik build preview && vite preview --open",
		"serve": "wrangler pages dev ./dist",
		"start": "vite --open --mode ssr",
		"pages:dev": "wrangler pages dev --compatibility-date=2023-12-18 -- npm run dev",
		"pages:deploy": "npm run build && wrangler pages deploy ./dist",
		"qwik": "qwik",
		"generate-admin": "graphql-codegen --config codegen-admin.ts",
		"generate-shop": "graphql-codegen --config codegen-shop.ts"
	},
	"devDependencies": {
		"@builder.io/qwik": "^1.3.1",
		"@builder.io/qwik-city": "^1.3.1",
		"@builder.io/qwik-react": "0.5.0",
		"@emotion/react": "^11.11.1",
		"@emotion/styled": "^11.11.0",
		"@graphql-codegen/cli": "5.0.0",
		"@graphql-codegen/introspection": "4.0.0",
		"@graphql-codegen/typescript": "^4.0.1",
		"@mui/material": "^5.13.0",
		"@mui/x-data-grid": "^6.4.0",
		"@types/eslint": "^8.44.8",
		"@types/node": "^20.10.6",
		"@types/react": "^18.2.46",
		"@types/react-dom": "^18.2.13",
		"@typescript-eslint/eslint-plugin": "^6.13.2",
		"@typescript-eslint/parser": "^6.13.2",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.55.0",
		"eslint-plugin-qwik": "^1.3.1",
		"postcss": "^8.4.32",
		"prettier": "^3.1.0",
		"prettier-plugin-tailwindcss": "^0.5.10",
		"qwik-speak": "^0.19.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"tailwindcss": "^3.4.0",
		"typescript": "^5.3.2",
		"undici": "^5.26.0",
		"vite": "^5.0.6",
		"vite-tsconfig-paths": "^4.2.3",
		"wrangler": "^3.0.0"
	},
	"dependencies": {
		"@fontsource-variable/public-sans": "^5.0.17",
		"@fontsource/source-sans-pro": "^5.0.8",
		"@graphql-codegen/typescript-generic-sdk": "^4.0.0",
		"@graphql-codegen/typescript-operations": "^4.0.1",
		"@modular-forms/qwik": "^0.22.0",
		"@portabletext/react": "^3.0.11",
		"@qwik-ui/headless": "^0.2.0",
		"@qwikest/icons": "^0.0.13",
		"@sanity/client": "^6.10.0",
		"@sanity/image-url": "^1.0.2",
		"@sanity/types": "^3.23.4",
		"@stripe/stripe-js": "^2.2.2",
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/typography": "^0.5.10",
		"class-variance-authority": "^0.7.0",
		"graphql": "^16.8.1",
		"graphql-tag": "^2.12.6",
		"groq": "^3.23.4",
		"npm-check": "^6.0.1",
		"postcss-value-parser": "^4.2.0",
		"qwik-image": "^0.0.8",
		"shiki": "^0.14.7",
		"zod": "^3.22.4"
	},
	"bin": {
		"autoprefixer": "bin/autoprefixer"
	},
	"peerDependencies": {
		"postcss": "^8.4.32"
	}
}
